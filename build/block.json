{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "roomworksmedia-block/roomworksmedia-forcast",
  "version": "0.1.0",
  "title": "Roomworksmedia Forcast",
  "category": "widgets",
  "icon": "media-interactive",
  "description": "An interactive block with the Interactivity API.",
  "example": {},
  "supports": {
    "interactivity": true,
    "color": {
      "gradients": true,
      "link": true,
      "text": true,
      "background": true
    },
    "spacing": {
      "padding": true,
      "margin": true
    },
    "layout": true,
    "typography": {
      "fontSize": true,
      "fontStyle": true,
      "fontWeight": true,
      "lineHeight": true,
      "textDecoration": true,
      "textTransform": true,
      "__experimentalFontFamily": true,
      "__experimentalDefaultControls": {
        "fontSize": true,
        "fontAppearance": true,
        "textTransform": true
      }
    },
    "dimensions": {
      "minHeight": true
    },
    "__experimentalBorder": {
      "radius": true,
      "color": true,
      "style": true,
      "width": true,
      "__experimentalDefaultControls": {
        "radius": true,
        "color": true,
        "style": true,
        "width": true
      }
    },
    "__experimentalStyle": {
      "elements": {
        "h3": {
          "color": true,
          "typography": {
            "fontSize": true,
            "fontWeight": true
          }
        }
      }
    }
  },
  "attributes": {
    "location": {
      "type": "string",
      "default": "London"
    },
    "unit": {
      "type": "string",
      "default": "C"
    },
    "speedUnit": {
      "type": "string",
      "default": "km/h"
    },
    "extraInfo": {
      "type": "boolean",
      "default": false
    },
    "backgroundColour": {
      "type": "string",
      "default": "#f3f3f3"
    },
    "iconPosition": {
      "type": "string",
      "default": "right"
    },
    "style": {
      "type": "object",
      "default": {}
    }
  },
  "textdomain": "roomworksmedia-forcast",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}